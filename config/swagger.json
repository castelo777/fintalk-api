{
    "swagger":"2.0",
    "info":{
        "description":"This api it's a simple auth and list users",
        "version":"1.0",
        "title":"FINTALK - API",
        "termsOfService":"http://swagger.io/terms/",
        "contact":{
            "email":"josecastelo02@gmail.com"
        }
    },
    "host":"localhost:3000",
    "basePath":"/",
    
    "schemes":[
        "http"
    ],
    "paths":{
        "/sign_in":{
            "post":{
                "tags":[
                    "Auth"
                ],
                "summary":"Login user in plataform",
                "description":"Login user in plataform",
                "consumes":[
                    "application/json"
                ],
                "produces":[
                    "application/json"
                ],
                "parameters":[
                    {
                        "in":"body",
                        "name":"body",
                        "description":"Auth object that needs to be confirm in plataform",
                        "required":true,
                        "schema":{
                            "$ref":"#/definitions/Auth"
                        }
                    }
                ],
                "responses":{
                    "500":{
                        "description":"Invalid input"
                    }
                }
            }
        },
        "/sign_up":{
            "post":{
                "tags":["Auth"],
                "summary":"Sign up into plataform",
                "description":"Sign up into plataform",
                "consumes":[
                    "application/json"
                ],
                "produces":[
                    "application/json"
                ],
                "parameters":[
                    {
                        "in":"body",
                        "name":"body",
                        "description":"Auth object that needs to be confirm in plataform",
                        "required":true,
                        "schema":{
                            "$ref":"#/definitions/UserCreate"
                        }
                    }
                ],
                "responses":{
                    "500":{
                        "description":"Invalid input"
                    }
                }
            }
        },
        "/user":{
            "get":{
                "tags":[
                    "User"
                ],
                "summary":"List user by filter params",
                "description":"Returns a list user",
                "produces":[
                    "application/json"
                ],
                "parameters":[
                    {
                        "name":"name",
                        "in":"query",
                        "description":"query filter users",
                        "required":true,
                        "type":"string"
                    },
                    {
                        "name":"phones",
                        "in":"query",
                        "description":"query filter users",
                        "required":true,
                        "type":"string"
                    },
                    {
                        "name":"email",
                        "in":"query",
                        "description":"query filter users",
                        "required":true,
                        "type":"string"
                    }
                ],
                "responses":{
                    "200":{
                        "description":"successful operation",
                        "schema":{
                            "type":"array",
                            "items":{
                                "$ref":"#/definitions/User"
                            }
                        }
                    },
                    "500":{
                        "description":"Internal Server Error"
                    },
                    "404":{
                        "description":"User list filter not found"
                    },
                    "401":{
                        "description":"Unauthorized please check your credentials"
                    }
                },
                "security":[
                    {
                        "api_key":[
                            "Bearer"
                        ]
                    }
                ]
            }
        },
        "/user/{id}":{
            "get":{
                "tags":[
                    "User"
                ],
                "summary":"Find user by id",
                "description":"Find user by id",
                "produces":[ "application/json" ],
                "parameters":[
                    {
                        "name":"id",
                        "in":"path",
                        "description":"ID of user that needs to be fetched",
                        "required":true,
                        "type":"string"
                    }
                ],
                "responses":{
                    "200":{
                        "description":"successful operation",
                        "schema":{
                            "$ref":"#/definitions/User"
                        }
                    },
                    "400":{
                        "description":"Invalid ID supplied"
                    },
                    "404":{
                        "description":"Order not found"
                    },
                    "500":{
                        "description":"Internal Server Error"
                    },
                    "401":{
                        "description":"Unauthorized please check your credentials"
                    }
                },
                "security":[
                    {
                        "api_key":[
                            "Bearer"
                        ]
                    }
                ]
            }
        }
    },
    "securityDefinitions":{
        "Bearer":{
            "type":"apiKey",
            "name":"Authorization",
            "in":"header"
        }
    },
    "definitions":{
        "Auth":{
            "type":"object",
            "required":[
                "email",
                "password"
            ],
            "properties":{
                "email":{
                    "type":"string"
                },
                "password":{
                    "type":"string"
                }
            },
            "xml":{
                "name":"Auth"
            }
        },
        "User":{
            "type":"object",
            "required":[
                "email",
                "phones",
                "name",
                "password"
            ],
            "properties":{
                "id":{
                    "type":"string"
                },
                "name":{
                    "type":"string"
                },
                "email":{
                    "type":"string"
                },
                "password":{
                    "type":"string"
                },
                "phones":{
                    "type":"array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "number":{
                                "type":"string"
                            },
                            "code":{
                                "type":"string"
                            }
                        }
                    }
                }
            },
            "xml":{
                "name":"User"
            }
        },
        "UserCreate":{
            "type":"object",
            "required":[
                "email",
                "phones",
                "name",
                "password"
            ],
            "properties":{
                "name":{
                    "type":"string"
                },
                "email":{
                    "type":"string"
                },
                "password":{
                    "type":"string"
                },
                "phones":{
                    "type":"array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "number":{
                                "type":"string"
                            },
                            "code":{
                                "type":"string"
                            }
                        }
                    }
                }
            },
            "xml":{
                "name":"User"
            }
        }
    },
    "externalDocs":{
        "description":"Find out more about Swagger",
        "url":"http://swagger.io"
    }
}